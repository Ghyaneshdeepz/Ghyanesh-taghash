generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model people {
  id            Int      @id @default(autoincrement())
  name          String
  gender        String
  birthdate     DateTime @db.Date
  is_vaccinated Boolean?
  vaccine_name  String?
}
